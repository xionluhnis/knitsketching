ype":"pickup","useIncreaseStitchNumber":true,"useSRTucks":false,"useSVS":false,"gauge":"half","subdiv":1,"stitchProgram":"const left = Action.register({\n  main: Action.knit,\n  post: ({ move }) => move(-1)\n});\nconst right = Action.register({\n  main: Action.knit,\n  post: ({ move }) => move(1)\n});\nconst purl = Action.register({\n  pre: ({ k, n, rn }) => k.xfer(n, rn),\n  main: ({ k, d, rn, cs }) => k.knit(d, rn, cs),\n  post: ({ k, rn, n }) => k.xfer(rn, n),\n  splitBySide: true\n});\nconst twoInches = prog.lengthToWaleStitches('2 in');\n\n// neck edge\nconst neckLen = twoInches + twoInches % 4;\nconst neckEdge = prog.filter(s => s.countNextWales() === 0);\nconst neck = neckEdge.left(2).waleGrid(0:end, -neckLen);\nneck.tile(0b10, 2).prog(purl);\n// sub-ribs\nfor(let i = 1, ci = 0, pass = 1; i < neckLen - 1; ++i){\n  const crs = prog.courses(-2 - ci).pass(pass);\n  const cl = crs.left();\n  const cr = crs.right();  \n  // go over wales\n  const seeds = prog.withIndices([]);\n  for(let si = 0; si < crs.indices.length; ++si){\n    const s = crs.stitches[crs.indices[si]];\n    const l = cl.stitches[cl.indices[si]];  \n    const r = cr.stitches[cr.indices[si]];\n    if(!s.getProgram()\n    && !l.getProgram()\n    && !r.getProgram()){\n      prog.withIndices([s.index]).prog(purl);\n      seeds.indices.push(s.index);\n    }\n  }\n  seeds.waleGrid(0:end, -(neckLen - i)).prog(purl);\n  // seeds.waleGrid(0:end, neckLen - i).prog(purl);\n  pass = 1 - pass;\n  if(pass) ++ci;\n}\n\n// trunk base\nconst bot = prog.node(0).filter(s => s.countPrevWales() === 0).up();\n//bot.tile(0b1001, 2).prog(purl);\nbot.waleGrid(0:end, 3).tile(0b1001, 2).prog(purl);\n// march up to create waffle pattern\nfor(let i = 0, crs = 0; i < neckLen; i += 4, crs += 2){\n  const lines = prog.courses(9 + crs);\n  // knit line (nothing to do)\n  // purl line (purl all)\n  lines.pass(0).prog(purl);\n  // garter bottom\n  const garter = prog.courses(10 + crs);\n  garter.pass(0).waleGrid(0:end, 2).tile(0b1001, 2).prog(purl);\n}\n\n\n// wrists\nconst wriPat = prog.strToGrid(`\nkkppkk\nkkppkk\npkppkp\npkppkp\npkkkkp\npkkkkp\n`);\nconst wriLen = Math.max(\n  wriPat.length * 2,\n\ttwoInches - twoInches % wriPat.length\n);\nconst wri = prog.node(1).or(prog.node(2)).filter(s => s.countPrevWales() === 0).waleGrid(0:end, wriLen).tileMap(wriPat, {\n  k: Action.Knit,\n  p: purl\n});\n\n\n// ---------------------------------------------------\n// cat colorwork -------------------------------------\n// ---------------------------------------------------\n\nconst cs2 = ['2'];\n\n// main cat region\nconst back = Action.register({\n  main: [\n    ({ k, d, n, cs }) => k.knit(d, n, cs),\n    ({ k, d, n }) => k.miss(d, n, cs2)\n  ],\n  splitBySide: true\n});\nconst front = Action.register({\n  main: [\n    ({ k, d, n, cs }) => k.miss(d, n, cs),\n    ({ k, d, n }) => k.knit(d, n, cs2)\n  ],\n  splitBySide: true\n});\nconst [dark, light] = [front, back];\n\n// secondary foreground region (for tucks)\nconst tuck = Action.register({\n  main: [\n    ({ k, d, n, e, cs }) => e.actIdx % 5 === 2 ? k.tuck(d, n, cs) : k.miss(d, n, cs),\n    ({ k, d, n }) => k.knit(d, n, cs2)\n  ],\n  splitBySide: true\n});\n\n// second yarn handling\nconst yarnIn = back.extend({\n  pre: ({ k, d, n, e }) => {\n    k.inhook(cs2);\n    k.tuck(d, n, cs2);\n    k.tuck(d, e.stepNeedle(2), cs2);\n    k.tuck(-d, e.stepNeedle(1), cs2);\n    k.releasehook(cs2);\n  }\n});\nconst yarnOut = back.extend({\n  post: ({ k }) => k.outhook(cs2)\n});\n \nconst caturl = 'data:image/png;base64,';\n\n// center stitch on front\nconst cs = prog.sketch(0).nearPosition({ x: 0, y: 150 });\nconst bl = cs.down(32).left(66);\n\nconst rows = bl.stitchGrid(Infinity, 100);\nconst img = prog.parseImage(caturl);\nrows.prog(purl)\nconst cat = rows.tileMap(img, {\n  0: dark,\n  255: light\n}, 46, 3, 0); \n\n// replace tiling remainder with a simpler one\nif(rows.maxWidth){\n  const rw = rows.maxWidth % 46;\n  if(rw){\n    const rem = bl.stitchGrid(Infinity, 1).waleGrid([-rw-1,-2], 100);\n    rem.tileMap(0b1001, {\n      0: front, 1: back\n    }, 2);\n    rem.filter(s => noise.simplex2(s.index * 2, 0) > 0).prog(front);\n    rem.filter(s => noise.simplex2(s.index * 2, 0) <= 0).prog(back);\n    \n    // borders\n    bl.stitchGrid(Infinity, 1).waleGrid([-rw-1,-rw+1], 100).tileMap([[0, 1, 0]], { 0: front, 1: back });\n    bl.stitchGrid(Infinity, 1).waleGrid([-4,-1], 100).tileMap([[0, 1, 0]], { 0: front, 1: back });\n  }\n}\n\n// yarn handling\nrows.first().prog(yarnIn);\n// rows.first().up().prog(yarnRelease);\nrows.last().prog(yarnOut);","sizing":{"default":{"wale":"135 mm / 100 stitches","course":"300 mm / 100 stitches"},"sketch":{"type":"sketch","scale":"4mm/5px"}},"carriers":{"1":{"color":"#9999FF"},"2":{"color":"#33FF66"},"3":{"DSCS":true,"color":"#FF9999"},"7":{"type":"elastic","color":"#FFFFFF"},"13":{"type":"plating","carriers":["1","3"]},"32":{"type":"inlay","carriers":["3","2"]},"default":{"type":"knit","DSCS":false,"carriers":["1"],"color":"#9999FF"}}},"resources":{},"ui":{"verbose":true,"expert_mode":false,"wasm_debug":false,"load_globals":false,"showMesh":"none","robust_meshing":true,"showColorFaces":true,"showGeodesicConnectivity":false,"showFullIsolines":true,"showBG":true,"showLinks":true,"showBeds":true,"showFlow":true,"showFlowConstraints":true,"showKappaConstraints":true,"showSeams":true,"showIsolines":true,"showPCurves":true,"showLabels":true,"cacheHIT":true,"mesh_profile":"custom","mesh_levels":false,"level_factor":"2","min_resolution":false,"nh_power":false,"nh_threshold":false,"geodesic_mode":"heat","geodesic_refinement":true,"refinement_threshold":false,"flow_accuracy":false,"time_accuracy":false,"time_stretch_range":false,"time_moment":false,"constr_support":false,"max_time_iter":false,"dtime_equation":"2","shaping_factor":false,"global_shaping":false,"global_alias":false,"global_budget":false,"local_budget":false,"unif_branching":false,"mixed_shaping":false,"min_wale_diff":false,"sr_mode":